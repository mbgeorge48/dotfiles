{
    "[django-html]": {
        "editor.formatOnSave": false
    },
    "[python]": {
        "editor.defaultFormatter": "ms-python.black-formatter"
    },
    "[yaml]": {
        "editor.defaultFormatter": "redhat.vscode-yaml"
    },
    "[shellscript]": {
        "editor.formatOnSave": false,
        "editor.defaultFormatter": "foxundermoon.shell-format"
    },
    "application.shellEnvironmentResolutionTimeout": 60,
    "catppuccin.bracketMode": "rainbow",
    "editor.codeActionsOnSave": {
        "source.fixAll": "explicit"
    },
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.fontFamily": "Jetbrains Mono, Hack, Inconsolata-Regular, Consolas, 'Courier New', monospace",
    "editor.fontLigatures": false,
    "editor.fontSize": 14,
    "editor.formatOnSave": true,
    "editor.guides.bracketPairs": true,
    "editor.largeFileOptimizations": true,
    "editor.minimap.autohide": true,
    "editor.minimap.enabled": true,
    "editor.minimap.renderCharacters": false,
    "editor.renderControlCharacters": true,
    "editor.renderWhitespace": "all",
    "editor.tabSize": 4,
    "emmet.triggerExpansionOnTab": true,
    "extensions.autoUpdate": "onlyEnabledExtensions",
    "files.exclude": {
        ".pytest_cache": true,
        "**/__pycache__": true,
        "**/coverage": true,
        "env": true,
        "htmlcov": true,
        "**/node_modules/": true,
        "*node_modules*": true,
        "*.tsx.html": true,
        ".cache": true,
        "**/._*": true
    },
    "files.associations": {
        "**/templates/*.html": "django-html",
        "**/templates/*": "django-txt"
    },
    "git.autofetch": true,
    "git.closeDiffOnOperation": true,
    "git.confirmSync": false,
    "git.enableSmartCommit": false,
    "githubPullRequests.terminalLinksHandler": "github",
    "git.openDiffOnClick": false,
    "html.format.indentInnerHtml": true,
    "html.format.preserveNewLines": false,
    "python.defaultInterpreterPath": "${workspaceFolder}/.venv/bin/python",
    "prettier.tabWidth": 4,
    "security.workspace.trust.untrustedFiles": "newWindow",
    "terminal.external.osxExec": "Terminal.app",
    "terminal.external.windowsExec": "C:\\Windows\\System32\\WindowsPowerShell\\v1.0\\powershell",
    "terminal.integrated.defaultProfile.osx": "zsh",
    "terminal.integrated.defaultProfile.windows": "Command Prompt",
    "terminal.integrated.enableMultiLinePasteWarning": "auto",
    "terminal.integrated.scrollback": 200000,
    "terminal.integrated.fontFamily": "Jetbrains Mono, Hack, Inconsolata-Regular, Consolas, 'Courier New', monospace",
    "terminal.integrated.fontSize": 13,
    "workbench.colorTheme": "Andromeda",
    "workbench.colorCustomizations": {
        "[*]": {
            "activityBar.border": "#131313",
            "activityBarBadge.background": "#ff00ff",
            "activityBarBadge.foreground": "#ffffff",
            "activityBar.inactiveForeground": "#ffffff",
            "activityBar.foreground": "#ffffff",
            "activityBar.background": "#131313",
            "titleBar.activeForeground": "#ffffff",
            // "titleBar.activeBackground": "#131313",
            "badge.background": "#ff00ff",
            "badge.foreground": "#ffffff",
            "editorGroup.dropBackground": "#131313aa",
            "editorGroupHeader.tabsBackground": "#131313aa",
            "editorGroupHeader.tabsBorder": "#131313aa",
            "editorLineNumber.activeForeground": "#ff00ff",
            "editorLineNumber.foreground": "#f3f3f3",
            "icon.foreground": "#ffffff",
            "input.background": "#131313",
            "input.foreground": "#ffffff",
            "gitDecoration.addedResourceForeground": "#ff00ff",
            "gitDecoration.conflictingResourceForeground": "#ff3333",
            "gitDecoration.modifiedResourceForeground": "#00ff66",
            "gitDecoration.untrackedResourceForeground": "#0066ff",
            "list.errorForeground": "#ff3333",
            "list.inactiveSelectionBackground": "#131313aa",
            "list.inactiveSelectionForeground": "#FFA500",
            "list.warningForeground": "#ffff33",
            "minimap.background": "#131313",
            "minimap.errorHighlight": "#ff3333",
            "minimap.findMatchHighlight": "#ff00ff55",
            "minimap.selectionHighlight": "#ffffff55",
            "minimap.warningHighlight": "#ffff33",
            "scrollbar.shadow": "#131313",
            "scrollbarSlider.background": "#131313aa",
            "scrollbarSlider.hoverBackground": "#ffffffaa",
            "sideBar.border": "#131313",
            "sideBar.foreground": "#ffffff",
            "tab.activeForeground": "#ffffff",
            "tab.inactiveForeground": "#a0a0a0",
            "terminal.background": "#131313",
            "terminal.foreground": "#ffffff",
            "tree.indentGuidesStroke": "#ff6600",
            "widget.border": "#ffffff"
        }
    },
    "workbench.iconTheme": "vscode-icons",
    "workbench.startupEditor": "readme",
    "workbench.tree.indent": 16,
    "diffEditor.ignoreTrimWhitespace": false,
    "scm.showChangesSummary": false,
    "scm.showOutgoingChanges": "always",
    "scm.showIncomingChanges": "always",
    "githubPullRequests.queries": [
        {
            "label": "Waiting For My Review",
            "query": "is:open review-requested:${user}"
        },
        {
            "label": "Assigned To Me",
            "query": "is:open assignee:${user}"
        },
        {
            "label": "Created By Me",
            "query": "is:open author:${user}"
        }
    ],
    "githubPullRequests.pullBranch": "never",
    "workbench.sideBar.location": "right",
    "peacock.favoriteColors": [
        {
            "name": "Angular Red",
            "value": "#dd0531"
        },
        {
            "name": "Azure Blue",
            "value": "#007fff"
        },
        {
            "name": "JavaScript Yellow",
            "value": "#f9e64f"
        },
        {
            "name": "Mandalorian Blue",
            "value": "#1857a4"
        },
        {
            "name": "Node Green",
            "value": "#215732"
        },
        {
            "name": "React Blue",
            "value": "#61dafb"
        },
        {
            "name": "Something Different",
            "value": "#832561"
        },
        {
            "name": "Svelte Orange",
            "value": "#ff3d00"
        },
        {
            "name": "Vue Green",
            "value": "#42b883"
        }
    ]
}
